    <mesh name="left_hand_link" file="left_hand_link.STL"/>
    <mesh name="AR1" file="AL1.STL" scale="1 -1 1"/>
    <mesh name="AR2" file="AL2.STL" scale="1 -1 1"/>
    <mesh name="AR3" file="AL3.STL" scale="1 -1 1"/>
