<!-- T1 serial model adapted from: https://github.com/BoosterRobotics/booster_gym/blob/main/resources/T1/T1_serial.xml -->

<mujoco model="T1">
  <compiler angle="radian" meshdir="meshes/"/>

  <default>
    <!-- general defaults -->
    <equality solref="0.001 2" />

    <!-- joint defaults -->
    <joint pos="0 0 0" limited="true" damping="0.01" armature="0.01" frictionloss="0.1" />

    <!-- joint classes -->
    <default class="jXXS">
      <joint actuatorfrcrange="-7 7"/>
    </default>
    <default class="jXS">
      <joint actuatorfrcrange="-15 15"/>
    </default>
    <default class="jS">
      <joint actuatorfrcrange="-18 18"/>
    </default>
    <default class="jM">
      <joint actuatorfrcrange="-20 20"/>
    </default>
    <default class="jL">
      <joint actuatorfrcrange="-30 30"/>
    </default>
    <default class="jXL">
      <joint actuatorfrcrange="-45 45"/>
    </default>
    <default class="jXXL">
      <joint actuatorfrcrange="-60 60"/>
    </default>

    <!-- geometry classes -->
    <default class="visual">
      <geom type="mesh" contype="0" conaffinity="0" group="2" density="0"/>
    </default>
    <default class="collision">
      <geom group="3" material="transparent" priority="1" friction="1. 0.01 0.005"/>
    </default>
  </default>

  <asset>
    <material name="metal" rgba=".9 .95 .95 1"/>
    <material name="black" rgba="0 0 0 1"/>
    <material name="white" rgba="1 1 1 1"/>
