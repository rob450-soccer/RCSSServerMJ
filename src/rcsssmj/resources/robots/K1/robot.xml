    <mesh name="Left_Arm_3" file="Left_Arm_3.STL"/>
    <mesh name="Left_Arm_4" file="Left_Arm_4.STL"/>
    <mesh name="Right_Arm_1" file="Left_Arm_1.STL" scale="1 -1 1"/>
    <mesh name="Right_Arm_2" file="Left_Arm_2.STL" scale="1 -1 1"/>
    <mesh name="Right_Arm_3" file="Left_Arm_3.STL" scale="1 -1 1"/>
    <mesh name="Right_Arm_4" file="Left_Arm_4.STL" scale="1 -1 1"/>
    <mesh name="Left_Hip_Pitch" file="Left_Hip_Pitch.STL"/>
