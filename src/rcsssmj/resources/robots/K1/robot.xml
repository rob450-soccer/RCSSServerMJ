<!-- K1 22 DOF model adapted from: https://github.com/BoosterRobotics/booster_assets/blob/main/robots/K1/K1_22dof.xml -->

<mujoco model="K1">
  <compiler angle="radian" meshdir="meshes/"/>

  <default>
    <!-- joint defaults -->
    <joint pos="0 0 0" limited="true"/>

    <!-- joint classes -->
    <default class="jXXS">
      <joint actuatorfrcrange="-6 6" armature="0.002"/>
    </default>
    <default class="jXS">
      <joint actuatorfrcrange="-14 14" armature="0.001"/>
    </default>
    <default class="jS">
      <joint actuatorfrcrange="-20 20" armature="0.0565"/>
    </default>
    <default class="jM">
      <joint actuatorfrcrange="-30 30" armature="0.0478125"/>
    </default>
    <default class="jL">
      <joint actuatorfrcrange="-35 35" armature="0.0339552"/>
    </default>
    <default class="jXL">
      <joint actuatorfrcrange="-40 40" armature="0.095625"/>
    </default>

    <!-- geometry classes -->
    <default class="visual">
      <geom type="mesh" contype="0" conaffinity="0" group="2" density="0"/>
    </default>
    <default class="collision">
      <geom group="3" material="transparent" priority="1" solimp="0.90 0.95 0.001" solref="0.001 1"/>
    </default>
  </default>

  <asset>
    <material name="gray" rgba="0.4 0.4 0.4 1"/>
    <material name="team" rgba="0.4 0.4 0.4 1"/>
    <material name="transparent" rgba="0.75294 0.75294 0.75294 1"/>

    <mesh name="Trunk" file="Trunk.STL"/>
